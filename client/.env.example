# The environment this app is running in (development, staging, production)
REACT_APP_ENV=development

# The URL for the backend API server
REACT_APP_API_URL=http://localhost:3000

# The URL for the WebSocket game server
REACT_APP_WEBSOCKET_URL=ws://localhost:3001

# Public key for an analytics provider (e.g., Segment, PostHog)
ANALYTICS_KEY=

# Public key for a payment provider (e.g., Stripe)
PAYMENT_PROVIDER_API_KEY=

# The URL for the server that provides the provably fair seed
PROVABLY_FAIR_SERVER_URL=http://localhost:3000/provably-fair
